\begin{Shaded}
\begin{Highlighting}[]
\NormalTok{steps <-}\StringTok{ }\NormalTok{activity }\OperatorTok{%>%}\StringTok{ }\KeywordTok{select}\NormalTok{(steps, date) }\OperatorTok{%>%}\StringTok{ }\KeywordTok{group_by}\NormalTok{(date) }\OperatorTok{%>%}\StringTok{ }\KeywordTok{summarise}\NormalTok{(}\DataTypeTok{steps_per_day =} \KeywordTok{sum}\NormalTok{(steps))}

\KeywordTok{hist}\NormalTok{(steps}\OperatorTok{$}\NormalTok{steps_per_day, }\DataTypeTok{main =} \StringTok{"Total Number of Steps Taken per Day"}\NormalTok{, }\DataTypeTok{xlab =} \StringTok{"Steps Per Day"}\NormalTok{, }\DataTypeTok{ylab =} \StringTok{"Interval"}\NormalTok{, }\DataTypeTok{col =} \StringTok{"light blue"}\NormalTok{)}
