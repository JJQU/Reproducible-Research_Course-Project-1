\subsection{Imputig missing values}\label{imputig-missing-values}

\begin{Shaded}
\begin{Highlighting}[]
\KeywordTok{summary}\NormalTok{(}\KeywordTok{is.na}\NormalTok{(activity))}
\end{Highlighting}
\end{Shaded}

\begin{verbatim}
##    steps            date          interval      
##  Mode :logical   Mode :logical   Mode :logical  
##  FALSE:15264     FALSE:17568     FALSE:17568    
##  TRUE :2304
\end{verbatim}

\begin{Shaded}
\begin{Highlighting}[]
\NormalTok{activity[}\KeywordTok{is.na}\NormalTok{(activity)] <-}\StringTok{ }\KeywordTok{c}\NormalTok{(average}\OperatorTok{$}\NormalTok{average_steps)}
\NormalTok{activity_new <-}\StringTok{ }\NormalTok{activity}

\NormalTok{steps_new <-}\StringTok{ }\NormalTok{activity_new }\OperatorTok{%>%}\StringTok{ }\KeywordTok{select}\NormalTok{(steps, date) }\OperatorTok{%>%}\StringTok{ }\KeywordTok{group_by}\NormalTok{(date) }\OperatorTok{%>%}\StringTok{ }\KeywordTok{summarise}\NormalTok{(}\DataTypeTok{steps_per_day_new =} \KeywordTok{sum}\NormalTok{(steps))}

\KeywordTok{hist}\NormalTok{(steps_new}\OperatorTok{$}\NormalTok{steps_per_day_new, }\DataTypeTok{main =} \StringTok{"Total Number of Steps Taken per Day (New Dataset)"}\NormalTok{, }\DataTypeTok{xlab =} \StringTok{"Steps Per Day (New)"}\NormalTok{, }\DataTypeTok{ylab =} \StringTok{"Interval"}\NormalTok{, }\DataTypeTok{col =} \StringTok{"blue"}\NormalTok{)}
\end{Highlighting}
\end{Shaded}

\includegraphics{PA1_template_files/figure-latex/unnamed-chunk-4-1.pdf}

\begin{Shaded}
\begin{Highlighting}[]
\NormalTok{mean_dailysteps_new <-}\StringTok{ }\KeywordTok{mean}\NormalTok{(steps_new}\OperatorTok{$}\NormalTok{steps_per_day_new, }\DataTypeTok{na.rm =} \OtherTok{TRUE}\NormalTok{)}
\NormalTok{mean_dailysteps_new}
\end{Highlighting}
\end{Shaded}

\begin{verbatim}
